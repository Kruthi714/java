import java.util.*;

class Main {
    public static void main(String[] args) {
        String sb="madam";
        StringBuilder b=new StringBuilder(sb);
        b.reverse();
        boolean flag=true;
        for(int i=0;i<sb.length();i++)
        {
            char c=b.charAt(i);
            
            if(sb.charAt(i)==c)
            {
               flag=true;
            }
            else
            {
                flag=false;
                System.out.println("not a palindrome");
                break;
            }
            
        }
        if (flag) {
            System.out.println("It is a palindrome");
        }
    }
}
