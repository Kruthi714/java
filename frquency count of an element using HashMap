
import java.util.*;
class Main {
    public static void main(String[] args) {
        Map<Integer,Integer>mp=new HashMap<>();
        int arr[]={1,3,2,1,4,1};
        for(int i=0;i<arr.length;i++)
        {
            if(!mp.containsKey(arr[i]))
            {
                mp.put(arr[i],1);
            }
            else
            {
                mp.put(arr[i],mp.get(arr[i])+1); 
            }
        }
        
        System.out.println(mp);
        
            
         
    }
}
