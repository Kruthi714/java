import java.util.*;

public class Main
{
	public static void main(String[] args) {
		
		String s="hello";
		//length() and charAt()
		int l=s.length();
		System.out.println(l);
		for(int i=0;i<s.length();i++)
		{
		    System.out.println(s.charAt(i));
		}
		System.out.println(s.indexOf("l"));
		String p="wello";
		//contains() and compareTo()
		System.out.println(s.compareTo(p));
		System.out.println(p.contains("nnello"));
		System.out.println(p.contains("ello"));
		//startsWith and endsWith
		System.out.println(p.startsWith("we"));
		System.out.println(p.endsWith("we"));
		//toLowerCasease and toUpperCase
		String k="WORLD";
		System.out.println(k.toLowerCase());
		String x="kruthi";
		System.out.println(x.toUpperCase());
		//concat()
		System.out.println(s.concat(x));
	}
}
